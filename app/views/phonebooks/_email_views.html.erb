			<div class = "col-md-2"></div>
			<div class = "col-md-10">
				<p>
					<ul>
						<% emails = @contact.emails%>
						<% emails.each do |email|%>
						<li><%= email.address%></li>
						<% end %>
					</ul>
				</p>
			</div>
		</div>
	</div>
</div>