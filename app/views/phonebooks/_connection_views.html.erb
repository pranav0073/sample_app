

<div class ="row">
	<div class = "col-md-4"<strong><p><h3>Connections</h3></p></strong>
		<div class = "row">
			<div class = "col-md-2"></div>
			<div class = "col-md-10">
				<p>
					<ul>
						
						
							<% if !@contact[:fb].blank? %>
							<li><strong>Facebook-id</strong></li>
							<p><%= @contact[:fb] %></p>
							<% end %>
			
							<% if !@contact[:google_plus].blank? %>
							<li><strong>Google + </strong></li>
							<p><%= @contact[:google_plus] %></p>
							<% end %>
						
							<% if !@contact[:twitter].blank? %>
							<li><strong>Twitter</strong></li>
							<p><%= @contact[:twitter] %></p>
							<% end %>
						
							<% if !@contact[:github].blank? %>
							<li><strong>Github</strong></li>
							<p><%= @contact[:github] %></p>
							<% end %>

							<% connnection = @contact.other_connections%>
							<% connnection.each do |connnection|%>
								<li><strong><%= connnection[:connection_type] %></strong></li>
								<p><%= connnection[:connection_detail] %></p>
							<% end %>
							
					</ul>
				</p>
			</div>
		</div>
	</div>
</div>






